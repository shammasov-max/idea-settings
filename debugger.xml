<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="showLibraryStackFrames" value="true" />
      <option name="valueLookupDelay" value="200" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
          <stepping-filters>
            <stepping-filter url-pattern="redux-saga" />
          </stepping-filters>
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>